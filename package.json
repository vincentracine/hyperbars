{
  "name": "hyperbars",
  "version": "0.1.2",
  "description": "Compiles handlebar template to hyperscript so it can be used with virtual-dom",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "browserify src/hyperbars.js -o dist/hyperbars.js",
    "minify": "browserify src/hyperbars.js | uglifyjs -mc warnings=false > dist/hyperbars.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincentracine/hyperbars.git"
  },
  "keywords": [
    "hyperscript",
    "compile",
    "compiler",
    "html",
    "template",
    "mustache"
  ],
  "author": "Vincent Racine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vincentracine/hyperbars/issues"
  },
  "homepage": "https://github.com/vincentracine/hyperbars#readme",
  "dependencies": {
    "htmlparser2": "^3.10.0",
    "virtual-dom": "git+https://github.com/vincentracine/virtual-dom.git"
  },
  "devDependencies": {
    "colors": "^1.3.2"
  }
}
